{
  "name": "@reserve/database",
  "version": "0.0.1",
  "description": "> TODO: description",
  "author": "Serhii Romaniuk <serhii.romaniuk@ambisafe.com>",
  "homepage": "https://github.com/serhiiromaniuk/reserve#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "https://registry.yarnpkg.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serhiiromaniuk/reserve.git"
  },
  "scripts": {
    "build": "babel src -d dist --verbose --compact true --delete-dir-on-start",
    "db:migrate:make": "knex migrate:make rename-me --esm",
    "db:migrate:up": "npm run build && DATABASE_DIR=dist knex migrate:latest --esm",
    "db:migrate:down": "npm run build && DATABASE_DIR=dist knex migrate:down --esm",
    "db": "node dist/utils/script.js",
    "db:up": "node dist/utils/script.js up -d && node dist/utils/script.js ps",
    "db:down": "node dist/utils/script.js down -v"
  },
  "bugs": {
    "url": "https://github.com/serhiiromaniuk/reserve/issues"
  },
  "dependencies": {
    "@reserve/utils": "0.0.1",
    "aws-sdk": "^2.929.0",
    "dotenv": "^10.0.0",
    "knex": "^0.95.14",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/register": "^7.14.5"
  }
}
